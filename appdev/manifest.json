{
  "manifest_version": 2,
  "name": "Practical Startpage Dev",
  "short_name": "Practical Startpage",
  "version": "2.2.0",
  "description": "Flexible start page that makes your data available in a single view.",
  "homepage_url": "https://github.com/PD75/practical-startpage",
  "optional_permissions": [
    "http://ajax.googleapis.com/*"
  ],
  "permissions": [
    "topSites",
    "chrome://favicon/",
    "tabs",
    "bookmarks",
    "management",
    "history",
    "sessions",
    "storage"
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "startpage.html"
  },
  "options_ui": {
    "page": "chrome.optionsUI.html",
    "open_in_tab": true
  }
}
