{
  "manifest_version": 2,
  "name": "Practical Startpage Dev",
  "short_name": "Practical Startpage",
  "version": "2.4.1",
  "default_locale": "en",
  "description": "__MSG_c_ps_slogan__",
  "homepage_url": "https://github.com/PD75/practical-startpage",
  "optional_permissions": [
    "http://ajax.googleapis.com/*"
  ],
  "permissions": [
    "topSites",
    "chrome://favicon/",
    "tabs",
    "bookmarks",
    "management",
    "history",
    "sessions",
    "storage",
    "https://mail.google.com/mail/feed/atom"
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "startpage.html"
  },
  "options_ui": {
    "page": "chrome.optionsUI.html",
    "open_in_tab": true
  }
}
